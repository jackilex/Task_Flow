{
  "name": "project3",
  "version": "1.0.0",
  "description": "A project management application built with the MERN stack.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/_________",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiberjiber/project3.git"
  },
  "keywords": [
    "react",
    "productivity"
  ],
  "author": "Evan Boswood, Rachael Adu, Alex Saint Victor, Sabrina Hollett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jiberjiber/project3/issues"
  },
  "homepage": "https://github.com/jiberjiber/project3#readme",
  "dependencies": {
    "axios": "^0.20.0",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "config": "^3.3.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "fawn": "^2.1.5",
    "if-env": "^1.0.4",
    "joi": "^17.2.1",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.7",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.13",
    "nodemailer-sendgrid": "^1.0.3",
    "path": "^0.12.7",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
